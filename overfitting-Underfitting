(X, y, c='b', marker='o', label=\"Veri Noktaları\")\n","plt.xlabel(\"Çalışma Saatleri\")\n","plt.ylabel(\"Sınav Geçme Durumu\")\n","plt.title(\"Lojistik Regresyon\")\n","plt.legend(loc='upper left')\n","plt.grid(True)\n","\n","# Modelin tahminlerini gösterin\n","X_test_sorted = np.sort(X_test, axis=0)\n","y_pred_proba = model.predict_proba(X_test_sorted)\n","plt.plot(X_test_sorted, y_pred_proba[:, 1], color='r', linewidth=2, label=\"Model\")\n","plt.axvline(x=0, color='g', linestyle='--', label=\"Eşik Değeri (0.5)\")\n","plt.legend(loc='upper left')\n","\n","plt.show()\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":527},"id":"FfwTcFOnIJOS","executionInfo":{"status":"ok","timestamp":1695714794635,"user_tz":-180,"elapsed":905,"user":{"displayName":"Aysun Yıldırım","userId":"02548399988924965471"}},"outputId":"4089c2ee-1fdf-4df9-a3dc-460a935da21e"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stderr","text":["/usr/local/lib/python3.10/dist-packages/sklearn/utils/validation.py:1143: DataConversionWarning: A column-vector y was passed when a 1d array was expected. Please change the shape of y to (n_samples, ), for example using ravel().\n","  y = column_or_1d(y, warn=True)\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":[],"metadata":{"id":"mjJv-JmbQz77"},"execution_count":null,"outputs":[]}]}